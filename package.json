{
  "name": "@flame-ui/hooks",
  "version": "1.0.0",
  "description": "flame-ui hooks packages",
  "main": "lib/index.js",
  "module": "lib/esm/index.js",
  "typings": "lib/index.d.ts",
  "repository": "git@github.com:flame-ui/flame-ui-hooks.git",
  "author": "Nick <nikolaymuller@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "yarn clean && rollup -c",
    "clean": "rimraf lib",
    "test": "jest",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "prettier": "prettier --write '*/**/*.{js,ts,tsx,json,md}'",
    "pub": "yarn build && yarn publish",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "eslintConfig": {
    "extends": "@mullerstd"
  },
  "prettier": "@mullerstd/eslint-config/prettier",
  "dependencies": {
    "react": "^17.0.1"
  },
  "devDependencies": {
    "@mullerstd/eslint-config": "^1.2.3",
    "@testing-library/react": "^11.1.2",
    "@testing-library/react-hooks": "^3.4.2",
    "@types/jest": "^26.0.15",
    "@types/react": "^16.9.55",
    "eslint": "^7.12.1",
    "husky": "^5.0.0-beta.0",
    "jest": "^26.6.3",
    "pinst": "^2.1.1",
    "prettier": "^2.1.2",
    "react-test-renderer": "^17.0.1",
    "rollup": "^2.33.1",
    "rollup-plugin-typescript2": "^0.29.0",
    "ts-jest": "^26.4.4",
    "typescript": "^4.0.5"
  }
}
